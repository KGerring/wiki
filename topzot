    <title>Data and software citation deposit guide</title>
    <author>
      <name>Sabrina Granger</name>
      <uri>http://zotero.org/users/1126475</uri>
    </author>
    <id>http://zotero.org/groups/5018631/items/LD3MW7TA</id>
    <published>2023-04-06T09:06:44Z</published>
    <updated>2023-04-06T09:06:44Z</updated>
    <link rel="self" type="application/atom+xml" href="https://api.zotero.org/groups/5018631/items/LD3MW7TA?format=atom"/>
    <link rel="alternate" type="text/html" href="https://www.zotero.org/groups/fair-impact_t4.3/items/LD3MW7TA"/>
    <zapi:key>LD3MW7TA</zapi:key>
