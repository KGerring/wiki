          <tr class="rights">
            <th style="text-align: right">Rights</th>
            <td>http://creativecommons.org/licenses/by/4.0/</td>
          </tr>
          <tr class="date">
            <th style="text-align: right">Date</th>
            <td>2016-08-16</td>
          </tr>
          <tr class="extra">
            <th style="text-align: right">Extra</th>
            <td>DOI: 10.12688/f1000research.9206.1
Type: article</td>
          </tr>
          <tr class="accessDate">
            <th style="text-align: right">Accessed</th>
            <td>2023-04-06 09:03:12</td>
