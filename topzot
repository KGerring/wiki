    <link rel="alternate" type="text/html" href="https://www.zotero.org/groups/fair-impact_t4.3/items/4CCF8ZEV"/>
    <zapi:key>4CCF8ZEV</zapi:key>
    <zapi:version>31</zapi:version>
    <zapi:lastModifiedByUser>Sabrina Granger</zapi:lastModifiedByUser>
    <zapi:itemType>document</zapi:itemType>
